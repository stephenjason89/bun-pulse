{
	"repositoryUrl": "git@github.com:stephenjason89/bun-pulse.git",
	"branches": ["main", { "name": "develop", "prerelease": "beta" }],
	"plugins": [
		"@semantic-release/commit-analyzer",
		[
			"@semantic-release/release-notes-generator",
			{
				"preset": "conventionalcommits",
				"presetConfig": {
					"types": [
						{ "type": "feat", "section": "ğŸš€ Features" },
						{ "type": "fix", "section": "ğŸ› Bug Fixes" },
						{ "type": "perf", "section": "âš¡ Performance" },
						{ "type": "chore", "section": "ğŸ“¦ Chores" },
						{ "type": "docs", "section": "ğŸ“š Documentation" },
						{ "type": "style", "section": "ğŸ’„ Code Style" },
						{ "type": "refactor", "section": "ğŸ› ï¸ Refactoring" },
						{ "type": "test", "section": "âœ… Testing" },
						{ "type": "revert", "section": "âª Reverts" }
					]
				}
			}
		],
		"@semantic-release/changelog",
		"@semantic-release/github",
		[
			"@semantic-release/git",
			{
				"assets": ["CHANGELOG.md", "package.json"],
				"message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
			}
		],
		"@semantic-release/npm"
	]
}
